    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "url-loader": "^4.1.1",
    "vitest": "^3.0.9",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1"
  },
  "overrides": {
    "constate": {
      "react": "$react"
    },
    "react-day-picker": {
      "react": "$react"
    }
  }
}
