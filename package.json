    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.10.0",
    "npm": "^11.10.1",
    "playwright": "^1.58.1",
    "postcss-loader": "^8.2.1",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.16",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "url-loader": "^4.1.1",
    "vitest": "^4.0.18",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.5",
    "webpack-hot-middleware": "^2.26.1"
  },
  "overrides": {
    "constate": {
      "react": "$react"
    },
    "react-day-picker": {
      "react": "$react"
    }
  }
}
